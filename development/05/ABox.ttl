:manuscript_1 a memo:Manuscript ;
    memo:hasPart :text_1 , :gloss_a , :gloss_b , :gloss_c , :gloss_d , :gloss_e , :gloss_f .

:manuscript_2 a memo:Manuscript ;
    memo:hasPart :text_2 , :gloss_g , :gloss_h , :gloss_i , :gloss_l .

:manuscript_3 a memo:Manuscript ;
    memo:hasPart :text_3 , :gloss_m , :gloss_n .

:text_1 a memo:Text ;
    memo:hasEmbodiment :folio_1 , :folio_2 .

:text_2 a memo:Text ;
    memo:hasEmbodiment :folio_3 , :folio_4 .

:text_3 a memo:Text ;
    memo:hasEmbodiment :folio_5 , :folio_6 .

:gloss_a a memo:Gloss ;
    memo:hasEmbodiment :folio_1 ;
    memo:hasCreator :glossator_1 ;
    memo:cites :text_1 .

:gloss_b a memo:Gloss ;
    memo:hasEmbodiment :folio_1 ;
    memo:hasCreator :glossator_1 ;
    memo:refersTo :text_1 .

:gloss_c a memo:Gloss ;
    memo:hasEmbodiment :folio_1 ;
    memo:hasCreator :glossator_1 ;
    memo:annotates :gloss_a .

:gloss_d a memo:Gloss ;
    memo:hasEmbodiment :folio_2 ;
    memo:hasCreator :glossator_2 ;
    memo:refersTo :gloss_a .

:gloss_e a memo:Gloss ;
    memo:hasEmbodiment :folio_2 ;
    memo:hasCreator :glossator_2 ;
    memo:annotates :gloss_b , :gloss_c .

:gloss_f a memo:Gloss ;
    memo:hasEmbodiment :folio_2 ;
    memo:hasCreator :glossator_2 ;
    memo:cites :text_1 ;
    memo:refersTo :gloss_d ;
    memo:annotates :gloss_e .

:gloss_g a memo:Gloss ;
    memo:hasEmbodiment :folio_3 ;
    memo:hasCreator :glossator_3 ;
    memo:cites :manuscript_1 ;
    memo:refersTo :text_1 ;
    memo:annotates :text_2 .

:gloss_h a memo:Gloss ;
    memo:hasEmbodiment :folio_3 ;
    memo:hasCreator :glossator_3 ;
    memo:refersTo :gloss_e ;
    memo:annotates :gloss_f , :text_2 .

:gloss_i a memo:Gloss ;
