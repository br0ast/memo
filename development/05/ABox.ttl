    memo:hasCreator :glossator_4 ;
    memo:annotates :gloss_g .

:gloss_l a memo:Gloss ;
    memo:hasEmbodiment :folio_4 ;
    memo:hasCreator :glossator_4 ;
    memo:cites :gloss_h ;
    memo:annotates :gloss_i .

:gloss_m a memo:Gloss ;
    memo:hasEmbodiment :folio_5 ;
    memo:hasCreator :glossator_5 ;
