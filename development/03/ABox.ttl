@prefix : <https://w3id.org/irnerio/data/memo/> .
@prefix memo: <https://w3id.org/irnerio/ontology/memo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Linking to the main ontology
<https://w3id.org/irnerio/data/memo-abox> a owl:Ontology ;
   owl:imports <https://w3id.org/irnerio/ontology/memo> .

:manuscript_1 a memo:Manuscript ;
    memo:hasPart :gloss_a , :gloss_b , :gloss_c , :gloss_d , :gloss_e , :gloss_f , :text_1 .

:gloss_a a memo:Gloss ;
    memo:cites :text_1 .

:gloss_b a memo:Gloss ;
    memo:refersTo :text_1 .

:gloss_c a memo:Gloss ;
    memo:annotates :gloss_a .

:gloss_d a memo:Gloss ;
    memo:refersTo :gloss_a .

:gloss_e a memo:Gloss ;
    memo:annotates :gloss_b , :gloss_c .

:gloss_f a memo:Gloss ;
    memo:cites :text_1 ;
    memo:refersTo :gloss_d ;
    memo:annotates :gloss_e .

:text_1 a memo:Text .

:manuscript_2 a memo:Manuscript ;
    memo:hasPart :text_2 , :gloss_g , :gloss_h , :gloss_i , :gloss_l ;
    memo:hasTextualMetadata :incipit_2 , :explicit_2 .

:incipit_2 a memo:TextualMetadataInTime ;
    memo:cites :manuscript_1 .

:explicit_2 a memo:TextualMetadataInTime ;
    memo:relatesToManuscript :manuscript_2 ;
    memo:cites :gloss_g .

:text_2 a memo:Text .

:gloss_g a memo:Gloss ;
    memo:cites :manuscript_1 ;
    memo:refersTo :text_1 ;
    memo:annotates :text_2 .

:gloss_h a memo:Gloss ;
    memo:refersTo :gloss_e ;
    memo:annotates :gloss_f , :text_2 .

:gloss_i a memo:Gloss ;
    memo:annotates :gloss_g ;
    memo:cites :edition_volume_1 .

:gloss_l a memo:Gloss ;
    memo:cites :gloss_h ;
    memo:annotates :gloss_i ;
    memo:cites :edition_volume_2 .

:edition_volume_1 a memo:CriticalEditionVolume .

:edition_volume_2 a memo:CriticalEditionVolume .

:edition_1 a memo:CriticalEdition ;
    memo:hasRealization :edition_volume_1 ;
    memo:hasTitle "Edition One"^^rdfs:Literal .

:edition_2 a memo:CriticalEdition ;
    memo:hasRealization :edition_volume_2 ;
    memo:hasTitle "Edition Two"^^rdfs:Literal .

:manuscript_3 a memo:Manuscript ;
    memo:hasPart :gloss_m , :gloss_n , :text_3 ;
    memo:hasTextualMetadata :incipit_3 .

:incipit_3 a memo:TextualMetadataInTime ;
    memo:cites :manuscript_2 .

:gloss_m a memo:Gloss ;
    memo:annotates :text_3 ;
    memo:cites :gloss_l .

:gloss_n a memo:Gloss ;
    memo:annotates :gloss_m ;
    memo:cites :edition_volume_1 .

:text_3 a memo:Text .

:edition_volume_3 a memo:CriticalEditionVolume ;
    memo:cites :manuscript_1 , :manuscript_2, :manuscript_3 .

:edition_3 a memo:CriticalEdition ;
    memo:hasRealization :edition_volume_3 ;
    memo:hasTitle "Edition Three"^^rdfs:Literal .