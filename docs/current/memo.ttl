    memo:hasNumberOfColumns "1"^^xsd:positiveInteger .\r
\r
ex:verso_4 a memo:Verso ;\r
    memo:hasNumberOfColumns "2"^^xsd:positiveInteger .\r
\r
ex:verso_5 a memo:Verso ;\r
    memo:hasNumberOfColumns "2"^^xsd:positiveInteger .\r
\r
ex:verso_6 a memo:Verso ;\r
    memo:hasNumberOfColumns "2"^^xsd:positiveInteger .\r
\r
ex:binding_1 a memo:Binding ;\r
    dcterms:medium memo:parchment ;\r
    dcterms:extent ex:extent_1 .\r
\r
ex:binding_2 a memo:Binding ;\r
    dcterms:medium memo:parchment ;\r
    dcterms:extent ex:extent_2 .\r
\r
ex:folio_1 a memo:Folio ;\r
    dcterms:medium memo:paper ;\r
    dcterms:extent ex:extent_1 ;\r
    frbr:part ex:recto_1 , ex:verso_1 .\r
\r
ex:folio_2 a memo:Folio ;\r
    dcterms:medium memo:paper ;\r
    dcterms:extent ex:extent_1 ;\r
    frbr:part ex:recto_2 , ex:verso_2 .\r
\r
ex:folio_3 a memo:Folio ;\r
