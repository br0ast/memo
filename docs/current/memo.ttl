ex:recto_5 a memo:Recto ;\r
    memo:hasNumberOfColumns "2"^^xsd:positiveInteger .\r
\r
ex:recto_6 a memo:Recto ;\r
    memo:hasNumberOfColumns "2"^^xsd:positiveInteger .\r
\r
ex:verso_1 a memo:Verso ;\r
