@prefix : <https://w3id.org/irnerio/data/memo/> .
@prefix memo: <https://w3id.org/irnerio/ontology/memo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Linking to the main ontology
<https://w3id.org/irnerio/data/memo-abox> a owl:Ontology ;
   owl:imports <https://w3id.org/irnerio/ontology/memo> .

:book_1 a memo:Book ;
    memo:hasTitle "Vermischte Schriften" ;
    memo:hasCreator :author_I ;
    memo:cites :codex_1 , :codex_2 .

:author_I a memo:Person .

:recto_1 a memo:Recto .

:recto_2 a memo:Recto .

:recto_3 a memo:Recto .

:recto_4 a memo:Recto .

:recto_5 a memo:Recto .

:recto_6 a memo:Recto .

:verso_1 a memo:Verso .

:verso_2 a memo:Verso .

:verso_3 a memo:Verso .

:verso_4 a memo:Verso .

:verso_5 a memo:Verso .

:verso_6 a memo:Verso .

:folio_1 a memo:Folio ;
    memo:hasMaterial memo:paper ;
    memo:hasExtent :extent_1 ;
    memo:hasNumberOfColumns "2"^^xsd:nonNegativeInteger ;
    memo:hasPart :recto_1 , :verso_1 .

:folio_2 a memo:Folio ;
    memo:hasMaterial memo:paper ;
    memo:hasExtent :extent_1 ;
    memo:hasNumberOfColumns "1"^^xsd:nonNegativeInteger ;
    memo:hasPart :recto_2 , :verso_2 .

:folio_3 a memo:Folio ;
    memo:hasMaterial memo:parchment ;
    memo:hasExtent :extent_1 ;
    memo:hasNumberOfColumns "1"^^xsd:nonNegativeInteger ;
    memo:hasPart :recto_3 , :verso_3 .

:folio_4 a memo:Folio ;
    memo:hasMaterial memo:parchment ;
    memo:hasExtent :extent_1 ;
    memo:hasNumberOfColumns "2"^^xsd:nonNegativeInteger ;
    memo:hasPart :recto_4 , :verso_4 .

:folio_5 a memo:Folio ;
    memo:hasMaterial memo:parchment ;
    memo:hasExtent :extent_3 ;
    memo:hasNumberOfColumns "2"^^xsd:nonNegativeInteger ;
    memo:hasPart :recto_5 , :verso_5 .

:folio_6 a memo:Folio ;
    memo:hasMaterial memo:parchment ;
    memo:hasExtent :extent_3 ;
    memo:hasNumberOfColumns "2"^^xsd:nonNegativeInteger ;
    memo:hasPart :recto_6 , :verso_6 .

:extent_1 a memo:Extent ;
    memo:hasHeight "210"^^xsd:nonNegativeInteger ;
    memo:hasWidth "180"^^xsd:nonNegativeInteger .

:extent_2 a memo:Extent ;
    memo:hasHeight "180"^^xsd:nonNegativeInteger ;
    memo:hasWidth "140"^^xsd:nonNegativeInteger .

:extent_3 a memo:Extent ;
    memo:hasHeight "160"^^xsd:nonNegativeInteger ;
    memo:hasWidth "120"^^xsd:nonNegativeInteger .

:codex_1 a memo:Codex ;
    memo:hasIdentifier "001" ;
    memo:datesBackTo :timeinterval_1 ;
    memo:hasExtent :extent_1 ;
    memo:hasPart :binding_1 , :folio_1 , :folio_2 , :folio_3 , :folio_4 .

:binding_1 a memo:Binding ;
    memo:hasMaterial memo:parchment .

:codex_2 a memo:Codex ;
    memo:hasIdentifier "002" ;
    memo:datesBackTo :timeinterval_2 ;
    memo:hasExtent :extent_2 ;
    memo:hasPart :binding_2 , :folio_5 , :folio_6 .

:binding_2 a memo:Binding ;
    memo:hasMaterial memo:parchment .

:manuscript_1 a memo:Manuscript ;
    memo:hasTitle "Arbor cum Glossis Ioannis Phasellus" ;
    memo:hasPart :text_1 .

:manuscript_2 a memo:Manuscript ;
    memo:hasTitle "Epistula ad Barbalum" ;
    memo:hasPart :text_2 .

:manuscript_3 a memo:Manuscript ;
    memo:hasTitle "Fragmenta Decreti" ;
    memo:hasPart :text_3 .

:text_1 a memo:Text ;
    memo:hasEmbodiment :folio_1 , :folio_2 .

:text_2 a memo:Text ;
    memo:hasEmbodiment :folio_3 , :folio_4 .

:text_3 a memo:Text ;
    memo:hasEmbodiment :folio_5 , :folio_6 .

:timeinterval_1 a memo:TimeInterval ;
    memo:hasIntervalStartDate "1200"^^xsd:gYear ;
    memo:hasIntervalEndDate "1299"^^xsd:gYear .

:timeinterval_2 a memo:TimeInterval ;
    memo:hasIntervalStartDate "1300"^^xsd:gYear ;
    memo:hasIntervalEndDate "1499"^^xsd:gYear .