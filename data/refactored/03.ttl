# Linking to the main ontology
<https://w3id.org/irnerio/data/refactored/memo> a owl:Ontology ;
   owl:imports <https://w3id.org/irnerio/ontology/memo> .

:manuscript_1 a memo:Manuscript ;
    frbr:part :text_1 , :gloss_a , :gloss_b , :gloss_c , :gloss_d , :gloss_e , :gloss_f .

:gloss_a a memo:Gloss ;
    cito:cites :text_1 .

:gloss_b a memo:Gloss ;
    dcterms:relation :text_1 .

:gloss_c a memo:Gloss ;
    memo:annotates :gloss_a .

:gloss_d a memo:Gloss ;
    dcterms:relation :gloss_a .

:gloss_e a memo:Gloss ;
    memo:annotates :gloss_b , :gloss_c .

:gloss_f a memo:Gloss ;
    cito:cites :text_1 ;
    dcterms:relation :gloss_d ;
    memo:annotates :gloss_e .

:text_1 a memo:Text .

:manuscript_2 a memo:Manuscript ;
    frbr:part :text_2 , :gloss_g , :gloss_h , :gloss_i , :gloss_l ;
    memo:hasTextualMetadata :incipit_2 , :explicit_2 .

:incipit_2 a memo:TextualMetadataInTime ;
    cito:cites :manuscript_1 .

:explicit_2 a memo:TextualMetadataInTime ;
    cito:cites :gloss_g .

:text_2 a memo:Text .

:gloss_g a memo:Gloss ;
    cito:cites :manuscript_1 ;
    dcterms:relation :text_1 ;
    memo:annotates :text_2 .

:gloss_h a memo:Gloss ;
    dcterms:relation :gloss_e ;
    memo:annotates :gloss_f , :text_2 .

:gloss_i a memo:Gloss ;
    memo:annotates :gloss_g ;
    cito:cites :edition_1 .

:gloss_l a memo:Gloss ;
    memo:annotates :gloss_i ;
    cito:cites :gloss_h , :edition_2 .

:edition_1 a memo:CriticalEditionVolume ;
    dcterms:title "Edition One".

:edition_2 a memo:CriticalEditionVolume ;
    dcterms:title "Edition Two" .

:manuscript_3 a memo:Manuscript ;
    frbr:part :text_3 , :gloss_m , :gloss_n ;
    memo:hasTextualMetadata :incipit_3 .

:incipit_3 a memo:TextualMetadataInTime ;
    cito:cites :manuscript_2 .

:gloss_m a memo:Gloss ;
    memo:annotates :text_3 ;
    cito:cites :gloss_l .

:gloss_n a memo:Gloss ;
    memo:annotates :gloss_m ;
    cito:cites :edition_1 .

:text_3 a memo:Text .

:edition_3 a memo:CriticalEditionVolume ;
    cito:cites :manuscript_1 , :manuscript_2, :manuscript_3 ;
    dcterms:title "Edition Three" .