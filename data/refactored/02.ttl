# Linking to the main ontology
<https://w3id.org/irnerio/data/refactored/memo> a owl:Ontology ;
   owl:imports <https://w3id.org/irnerio/ontology/memo> .

:incipit_1 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:incipit ;
    memo:relatesToTextualContext :context_incipit_1 ;
    tvc:atTime :1779 .

:context_incipit_1 a owl:Thing ;
    memo:isBasedOn :text_1 , :gloss_a .

:1779 a ti:TimeInterval ;
    ti:hasIntervalStartDate "1779"^^xsd:gYear .

:text_1 a memo:Text .

:gloss_a a memo:Gloss .

:manuscript_1 a memo:Manuscript ;
    frbr:part :text_1 , :gloss_a , :gloss_f ;
    memo:hasTextualMetadata :incipit_1 , :explicit_1 , :f_rubric_1 .

:explicit_1 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:explicit ;
    memo:relatesToTextualContext :context_explicit_1 ;
    tvc:atTime :1780 .

:context_explicit_1 a owl:Thing ;
    memo:isBasedOn :text_1 , :gloss_f .

:gloss_f a memo:Gloss .

:1780 a ti:TimeInterval ;
    ti:hasIntervalStartDate "1780"^^xsd:gYear .

:f_rubric_1 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:finalRubric ;
    memo:relatesToTextualContext :context_f_rubric_1 ;
    tvc:atTime :1780 .

:context_f_rubric_1 a owl:Thing ;
    memo:isBasedOn :text_1 .

:incipit_2 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:incipit ;
    memo:relatesToTextualContext :context_incipit_2 ;
    tvc:atTime :1861 .

:context_incipit_2 a owl:Thing ;
    memo:isBasedOn :text_2 .

:1861 a ti:TimeInterval ;
    ti:hasIntervalStartDate "1861"^^xsd:gYear .

:text_2 a memo:Text .

:manuscript_2 a memo:Manuscript ;
    frbr:part :text_2 ;
    memo:hasTextualMetadata :incipit_2 , :explicit_2 .

:explicit_2 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:explicit ;
    memo:relatesToTextualContext :context_explicit_2 ;
    tvc:atTime :1861 .

:context_explicit_2 a owl:Thing ;
    memo:isBasedOn :text_2 .

:incipit_3 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:incipit ;
    memo:relatesToTextualContext :context_incipit_3 ;
    tvc:atTime :1800 .

:context_incipit_3 a owl:Thing ;
    memo:isBasedOn :text_3 .

:1800 a ti:TimeInterval ;
    ti:hasIntervalStartDate "1800"^^xsd:gYear .

:text_3 a memo:Text .

:manuscript_3 a memo:Manuscript ;
    frbr:part :text_3 ;
    memo:hasTextualMetadata :incipit_3 .