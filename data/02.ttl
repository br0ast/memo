# Linking to the main ontology
<https://w3id.org/irnerio/data/memo-abox> a owl:Ontology ;
   owl:imports <https://w3id.org/irnerio/ontology/memo> .

:incipit_1 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:incipit ;
    memo:relatesToTextualContext :context_incipit_1 ;
    memo:atTime :1779 .

:context_incipit_1 a owl:Thing ;
    memo:isBasedOn :text_1 , :gloss_a .

:1779 a memo:TimeInterval ;
    memo:hasIntervalStartDate "1779"^^xsd:gYear .

:text_1 a memo:Text .

:gloss_a a memo:Gloss .

:manuscript_1 a memo:Manuscript ;
    memo:hasPart :text_1 , :gloss_a , :gloss_f ;
    memo:hasTextualMetadata :incipit_1 , :explicit_1 , :f_rubric_1 .

:explicit_1 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:explicit ;
    memo:relatesToTextualContext :context_explicit_1 ;
    memo:atTime :1780 .

:context_explicit_1 a owl:Thing ;
    memo:isBasedOn :text_1 , :gloss_f .

:gloss_f a memo:Gloss .

:1780 a memo:TimeInterval ;
    memo:hasIntervalStartDate "1780"^^xsd:gYear .

:f_rubric_1 a memo:TextualMetadataInTime ;
    memo:withTextualRole memo:finalRubric ;
    memo:relatesToTextualContext :context_f_rubric_1 ;
    memo:atTime :1780 .

:context_f_rubric_1 a owl:Thing ;
    memo:isBasedOn :text_1 .

